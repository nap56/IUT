algo exo1TD2

var
	entier N, C, tmp;
	
principal
debut
	C:=0;
	saisir("Entrez un nombre entier:", @N);
	tantque
		repeter
			saisir("Entrez un entier:", @tmp);
			si (tmp==N) alors
				C:=C + 1;
			finsi
		jusqua (tmp==N)
		finrepeter
	sinon
		saisir("Entrez une valeur diff√©rente de -1:", @N);
	finsi
	afficher("Il y a " + C );
	afficher(" fois l'entier " + N );
	afficher(" .");
fin